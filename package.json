{
  "name": "@jirafa/monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.1.0",
  "description": "jirafa.js monorepo",
  "author": "linco",
  "license": "MIT",
  "homepage": "https://github.com/jirafa-js/jirafa",
  "keywords": [],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "cz": "czg",
    "preinstall": "only-allow pnpm -y",
    "prepare": "husky install",
    "changelog": "jirafa changelog",
    "gen": "jirafa create",
    "build": "jirafa build",
    "docs:dev": "pnpm run -F docs dev",
    "docs:serve": "pnpm run -F docs serve",
    "docs:build": "pnpm run -F docs build",
    "docs:gen-locale": "pnpm run -F docs gen-locale",
    "test": "vitest",
    "typecheck": "run-p typecheck:**",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:node": "vue-tsc -p tsconfig.node.json  --composite false  --noEmit",
    "typecheck:vitest": "vue-tsc -p tsconfig.vitest.json  --composite false  --noEmit",
    "bump": "bumpp --execute=\"jirafa bump\" --all"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@jirafa/components": "workspace:*",
    "@jirafa/hooks": "workspace:*",
    "@jirafa/icons": "workspace:*",
    "@jirafa/theme": "workspace:*",
    "@jirafa/token": "workspace:*",
    "@jirafa/utils": "workspace:*",
    "@vueuse/core": "^8.9.3",
    "async-validator": "^4.2.5",
    "lodash-unified": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@esbuild-kit/cjs-loader": "^2.4.0",
    "@jirafa/cli": "workspace:0.0.0",
    "@jirafa/eslint-config": "^1.1.2",
    "@pnpm/find-workspace-packages": "^4.0.43",
    "@pnpm/types": "^8.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^20.0.0",
    "@types/node": "18.8.0",
    "@types/prettier": "^2.7.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/test-utils": "^2.2.6",
    "bumpp": "^8.2.1",
    "csstype": "^3.1.1",
    "cz-git": "^1.3.11",
    "czg": "^1.3.11",
    "esbuild": "^0.14.54",
    "eslint": "^8.31.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jsdom": "16.4.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.1.1",
    "sass": "^1.55.0",
    "tsx": "^3.10.1",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "^0.21.2",
    "unplugin-vue-define-options": "^0.6.2",
    "vite": "^3.0.0",
    "vitest": "^0.26.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,md,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
